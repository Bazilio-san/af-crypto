{
  "name": "af-crypto",
  "version": "0.0.4",
  "description": "Colors",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "author": "Viacheslav Makarov <vvmakarov@bazilio.ru>",
  "license": "ISC",
  "keywords": [
    "colors"
  ],
  "productName": "AF Color",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "build": "tsc -b tsconfig.json tsconfig.esm.json tsconfig.types.json",
    "lint": "eslint . --ext .ts ",
    "lint:fix": "eslint . --ext .ts --fix",
    "pub": "npm run lint:fix && npm run clean && npm run build && npm publish",
    "cb": "npm run clean && npm run build"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/uuid": "^9.0.3",
    "@types/xxhashjs": "^0.2.2",
    "eslint-config-af-22": "^1.0.13",
    "shx": "^0.3.4",
    "typescript": "^5.2.2"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bazilio-san/af-crypto.git"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
